    <listOfRules>
      <assignmentRule variable="kpAB">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci> kpAA </ci>
            <ci> rA </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="kpBB">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
