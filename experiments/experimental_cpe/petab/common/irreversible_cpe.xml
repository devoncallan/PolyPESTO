            <ci> kpAA </ci>
            <ci> rX </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="kpBA">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci> kpBB </ci>
            <ci> rB </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="A">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> A0 </ci>
            <apply>
              <minus/>
              <cn type="integer"> 1 </cn>
              <ci> xA </ci>
            </apply>
